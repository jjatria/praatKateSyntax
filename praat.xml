    <contexts>
      <context attribute="Normal Text" lineEndContext="#pop" name="Normal Text" >
        <RegExpr context="#stay" attribute="Keyword" String='\bfor\b' beginRegion='forBlock' />
        <RegExpr context="#stay" attribute="Keyword" String='\bendfor\b' endRegion='forBlock' />
        <RegExpr context="#stay" attribute="Keyword" String='\bif\b' beginRegion='ifBlock' />
        <RegExpr context="#stay" attribute="Keyword" String='\bendif\b' endRegion='ifBlock' />
        <RegExpr context="#stay" attribute="Keyword" String='\bform\b' beginRegion='forRegion' />
        <RegExpr context="#stay" attribute="Keyword" String='\bendform\b' endRegion='forRegion' />
        <RegExpr context="#stay" attribute="Keyword" String='\bprocedure\b' beginRegion='procRegion' />
        <RegExpr context="#stay" attribute="Keyword" String='\bendproc\b' endRegion='procRegion' />
        <RegExpr context="#stay" attribute="Keyword" String='\bwhile\b' beginRegion='whileRegion' />
        <RegExpr context="#stay" attribute="Keyword" String='\bendwhile\b' endRegion='whileRegion' />
        <RegExpr context="#stay" attribute="Keyword" String='\brepeat\b' beginRegion='repeatRegion' />
        <RegExpr context="#stay" attribute="Keyword" String='\buntil\b' endRegion='untilRegion' />
        
        <StringDetect String='View &amp; Edit' attribute="Keyword" context="#stay" />

        <keyword    attribute="Commands"  context="commands"         String="commands" />
        <keyword    attribute="Keyword"   context="#stay"            String="keywords" />
        <keyword    attribute="Keyword"   context="#stay"            String="directives" />
        <keyword    attribute="Keyword"   context="stringDirectives" String="stringDirectives" />
        <keyword    attribute="Keyword"   context="objectDirectives" String="objectDirectives" />
        <keyword    attribute="Functions" context="#stay"            String="functions" />

        <DetectChar attribute="Comments"  context="comments"           char="#" />
        <DetectChar attribute="String"    context="string"             char="&quot;" />
        <DetectChar attribute="Normal Text"  context="expansion" char="'" />
      </context>

      <context name="string" attribute="String" lineEndContext="#pop" >
        <DetectChar attribute="String" context="#pop" char="&quot;" />
        <DetectChar attribute="Normal Text"  context="expansion" char="'" />
      </context>

      <context name="commands" attribute="Commands" lineEndContext="#pop" >
        <StringDetect attribute="Commands" context="commandArguments" String="..." />
      </context>
      
      <context name="commandArguments" attribute="Normal Text" lineEndContext="#pop" >
        <keyword    attribute="Keyword"   context="#stay"            String="methods" />
        <DetectChar attribute="Normal Text"  context="expansion" char="'" />
        <DetectChar attribute="String"    context="string"             char="&quot;" />
      </context>

      <context name="directives" attribute="Keyword" lineEndContext="#pop" >
      </context>

      <context name="stringDirectives" attribute="Keyword" lineEndContext="#pop" >
        <DetectChar attribute="String" context="string" char=' ' />
      </context>

      <context name="objectDirectives" attribute="Normal Text" lineEndContext="#pop" >
        <keyword    attribute="Object"  context="#stay"         String="objects" />
        <DetectChar attribute="Normal Text"  context="expansion" char="'" />
      </context>
      
      <context name="comments" attribute="Comments" lineEndContext="#pop" >
      </context>

      <context name="expansion" attribute="Expansion" lineEndContext="#pop" >
        <DetectChar attribute="Normal Text"  context="#pop"  char="'"/>
        <DetectChar attribute="String"  context="#pop#pop"  char='"'/>
      </context>

    </contexts>
    <itemDatas>
